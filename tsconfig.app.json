/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "module": "ES2022",
    "allowJs": true,
    "types": ["node"],
    "noImplicitAny": false,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
  },
  "files": [
    "src/main.ts"
  ],
  "include": [
    "src/**/*.spec.ts"
  ]
}
